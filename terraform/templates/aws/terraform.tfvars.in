aws_name_search = "@AWSNAMESEARCH@"
aws_virt_type = "@AWSVIRTTYPE@"
aws_ami_owner = "@AWSAMIOWNER@"
aws_instance_type = "@AWSINSTANCETYPE@"

limit_boxes = "@LIMITBOXES@"
limit_num_boxes = @LIMITNUMBOXES@

ssh_config_pubkey_file = "@SSHCONFIGPUBKEYFILE@"
